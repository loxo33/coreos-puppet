[Unit]
Description=<%= $description %>

<% $after.each |$afr| {-%>
After=<%= $afr %>
<% } -%>

<% $requires.each |$req| {-%>
Requires=<%= $req %>
<% } -%>

[Service]
<% $execstartpre.each |$exp| {-%>
ExecStartPre=<%= $exp %>
<% } -%>


ExecStart=<%= $execstart %>
RestartSec=<%= $restartsec %>
Restart=<%= $restart %>
<% if $wantedby {-%>
WantedBy=<%= $wantedby %>
<% } -%>
