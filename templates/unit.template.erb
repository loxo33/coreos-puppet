[Unit]
Description=<%= @unit_description %>
<% @unit_after.each do |after| -%>
After=<%= after %>
<% end -%>
<% @unit_requires.each do |requires| -%>
Requires=<%= requires %>
<% end -%>
[Service]
<% @execstartpre.each do |exp| -%>
ExecStartPre=<%= exp %>
<% end -%>
ExecStart=<%= @execstart %>
<% if @restartsec -%>RestartSec=<%= @restartsec %><% end %>
<% if @restart -%>Restart=<%= @restart %><% end %>
[Install]
<% if @unit_wantedby -%>WantedBy=<%= @unit_wantedby %><% end %>
